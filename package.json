{
  "private": true,
  "description": "Material Components Web",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run clean && mkdirp dist && npm run build:static && webpack --config=src/webpack.config.js --progress --colors --mode production",
    "build:static": "node \"$PWD/scripts/webpack/copy-static.js\"",
    "deploy": "npm run build && node scripts/deploy.js",
    "clean": "del-cli dist/** dist .closure-tmp/** .closure-tmp",
    "clean:site": "del-cli .site-generator-tmp/** .site-generator-tmp",
    "commitmsg": "validate-commit-msg",
    "dist": "npm run build && npm run build:min",
    "dev": "npm run clean && cross-env MDC_ENV=development webpack-dev-server --config=src/webpack.config.js --mode development --progress --inline --hot --host 0.0.0.0",
    "fix:js": "eslint --fix packages test webpack.config.js src/webpack.config.js karma.conf.js",
    "fix:css": "stylelint --fix \"packages/**/*.scss\"",
    "fix": "npm-run-all --parallel fix:*",
    "lint:js": "eslint packages test scripts webpack.config.js src/webpack.config.js karma.conf.js",
    "lint:css": "stylelint \"packages/**/*.scss\"",
    "lint:imports": "node scripts/check-imports.js",
    "lint": "npm-run-all --parallel lint:*",
    "postinstall": "lerna bootstrap",
    "pretest": "npm run lint",
    "test": "npm run test:unit && npm run test:closure && npm run test:dependency && npm run build && npm run clean",
    "test:watch": "karma start --auto-watch",
    "test:unit": "karma start --single-run",
    "test:closure": "./scripts/closure-test.sh",
    "test:dependency": "./scripts/dependency-test.sh",
    "test:site": "npm run clean:site && ./scripts/site-generator-test.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@google-cloud/storage": "^1.6.0",
    "@octokit/rest": "^15.9.4",
    "argparse": "^1.0.10",
    "ascii-table": "0.0.9",
    "autoprefixer": "^9.0.0",
    "awscred": "^1.4.1",
    "babel-loader": "^8.0.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-traverse": "^6.24.1",
    "babel-types": "^6.24.1",
    "babylon": "^6.16.1",
    "bel": "^6.0.0",
    "camel-case": "^3.0.0",
    "chai": "^4.0.2",
    "chokidar": "^2.0.4",
    "cli-table": "^0.3.1",
    "codecov": "^3.0.0",
    "colors": "^1.3.0",
    "cp-file": "^6.0.0",
    "cross-env": "^5.0.0",
    "css-loader": "^1.0.0",
    "cssom": "^0.3.2",
    "debounce": "^1.1.0",
    "del": "^3.0.0",
    "del-cli": "^1.0.0",
    "detect-port": "^1.2.3",
    "dom-events": "^0.1.1",
    "eslint": "^5.1.0",
    "eslint-config-google": "^0.8.1",
    "eslint-plugin-mocha": "^5.0.0",
    "express": "^4.16.3",
    "fs-extra": "^6.0.1",
    "glob": "^7.1.1",
    "handlebars": "^4.0.11",
    "husky": "^0.14.0",
    "is-online": "^7.0.0",
    "jimp": "^0.2.28",
    "json-loader": "^0.5.4",
    "json-stable-stringify": "^1.0.1",
    "lerna": "^2.11.0",
    "material-components-web": "^0.44.1",
    "mini-css-extract-plugin": "^0.5.0",
    "mkdirp": "^0.5.1",
    "mz": "^2.7.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.1",
    "os-name": "^2.0.1",
    "postcss-loader": "^2.0.3",
    "ps-node": "^0.1.6",
    "query-ast": "^1.0.1",
    "recast": "^0.15.0",
    "request-promise-native": "^1.0.5",
    "resolve": "^1.3.2",
    "sass-loader": "^7.1.0",
    "scss-parser": "^1.0.0",
    "semver": "^5.3.0",
    "serve-index": "^1.9.1",
    "simple-git": "^1.92.0",
    "stylelint": "^9.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^3.0.0",
    "stylelint-selector-bem-pattern": "^1.0.0",
    "testdouble": "^3.2.4",
    "to-slug-case": "^1.0.0",
    "useragent": "^2.3.0",
    "validate-commit-msg": "^2.6.1",
    "verror": "^1.10.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.11"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "transform-object-assign"
    ],
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  },
  "config": {
    "validate-commit-msg": {
      "helpMessage": "%s\nNOTE: Please see angular's commit message guidelines (https://github.com/angular/angular.js/blob/master/DEVELOPERS.md#commits) for information on how to format commit messages.\n\nAs an example, here is a valid commit message: 'docs(slider): Document slider public api'\n\nIf this commit is on a development / WIP branch, you can disable this by running `git commit --no-verify`.",
      "scope": {
        "allowed": [
          "animation",
          "auto-init",
          "base",
          "button",
          "card",
          "checkbox",
          "chips",
          "dialog",
          "drawer",
          "elevation",
          "fab",
          "floating-label",
          "form-field",
          "grid-list",
          "icon-button",
          "icon-toggle",
          "image-list",
          "layout-grid",
          "line-ripple",
          "linear-progress",
          "list",
          "menu",
          "notched-outline",
          "radio",
          "ripple",
          "rtl",
          "select",
          "selection-control",
          "shape",
          "slider",
          "snackbar",
          "switch",
          "tabs",
          "tab",
          "tab-bar",
          "tab-indicator",
          "tab-scroller",
          "text-field",
          "theme",
          "toolbar",
          "top-app-bar",
          "typography",
          "src",
          "infrastructure",
          "package"
        ],
        "validate": true
      }
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "closureWhitelist": [
    "mdc-animation",
    "mdc-base",
    "mdc-checkbox",
    "mdc-chips",
    "mdc-floating-label",
    "mdc-form-field",
    "mdc-icon-button",
    "mdc-icon-toggle",
    "mdc-line-ripple",
    "mdc-list",
    "mdc-menu",
    "mdc-notched-outline",
    "mdc-radio",
    "mdc-ripple",
    "mdc-select",
    "mdc-selection-control",
    "mdc-slider",
    "mdc-switch",
    "mdc-tab",
    "mdc-tab-indicator",
    "mdc-tab-scroller",
    "mdc-tab-bar",
    "mdc-textfield",
    "mdc-top-app-bar"
  ],
  "dependencies": {
    "cpx": "^1.5.0",
    "s3": "^4.4.0"
  }
}
